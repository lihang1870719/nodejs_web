<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">           
      <title><%= title %></title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="/stylesheets/navbar-static-top.css" />
    <script type="text/javascript" src="/javascripts/jquery-1.11.1.js"></script>
    <script type="text/javascript" src="/javascripts/admindelete.js"></script>
  </head>
  <body>
      <% include header %> 
      <div class="container">
         <div class="row">
            <table class="table table-hover table-bordered">
              <thead>
                <tr>
                <th>Name</th>
                <th>Title</th>
                <th>Introdunction</th>
                <th>View</th>
                <th>Update</th>
                <th>Delete</th>
                </tr>
              </thead>
              <tbody>
                <% user.forEach(function(user){ %>
                <tr class="user-id-<%= user.Id%>">
                  <td><%= user.Name %></td>
                  <td><%= user.Title %></td>
                  <td><%= user.Introduction %></td>
                  <td><a href="#">View</a></td>
                  <td><a href="#">Update</a></td>
                  <td><button type="button" class="btn btn-danger del" data-id="<%= user.Id%>">Delete</button></td>
                </tr>
                <% })  %>
              </tbody>
            </table>
         </div>
       </div>
  </body>
</html>